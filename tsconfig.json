{
  "compilerOptions": {
    "baseUrl": "./src",
    "composite": true,
    "esModuleInterop": true,
    "jsx": "react-jsx",
    "paths": {
      "@app/*": ["app/*"],
      "@assets/*": ["assets/*"],
      "@configs/*": ["configs/*"],
      "@css/*": ["assets/css/*"],
      "@mocks/*": ["mocks/*"],
      "@modules/*": ["modules/*"],
      "@share/*": ["share/*"]
    },
    "resolveJsonModule": true
  },
  "files": [],
  "references": [
    {
      "path": "./tsconfig.app.json"
    },
    {
      "path": "./tsconfig.node.json"
    }
  ]
}
